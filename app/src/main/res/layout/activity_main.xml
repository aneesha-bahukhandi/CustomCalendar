    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.android.bahukhandi.aneesha.calendar.customcalendar.MainActivity">

    <com.android.bahukhandi.aneesha.calendar.android_calendar_view.views.VerticalCalendar
